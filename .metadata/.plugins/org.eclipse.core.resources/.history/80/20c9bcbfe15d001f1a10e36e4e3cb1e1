package ex01;

import java.util.Random;

public class 로또2 {
	public static void main(String[] args) {
		//로또 -> 5게임
		
		int lotto[] = new int[6];
		//(int)(Math.random()*45) + 1;
		//random.nextInt{45)+1;
		
		Random random = new Random();
		
		
		for(int i=0; i< 5; i++) {
			for(int j=0; j<6; j++) {
				lotto[i] = random.nextInt(45)+1;
				
				for(int k=0; k<j; k++) {
					if(lotto[k] == lotto[i]) {
						j--;
					}
				
				
			}
			for(int x=0;x<6;x++)
				System.out.print(lotto[i] + ", ");
			System.out.println();
		}
			
		

	}

}
