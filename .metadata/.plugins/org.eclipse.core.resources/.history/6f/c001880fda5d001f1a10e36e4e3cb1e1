package ex01;

import java.util.Random;

public class 로또 {
	
	public boolean no_dup(int num[], int newnum) {
		for(int i=0; i<num.length; i++) {
			if(num[i] == newnum)
				return true;
			else
				return false;
		}
	}
	

	public static void main(String[] args) {
		int num[] = new int[5];
		char c[] = {'A', 'B', 'C', 'D', 'E'};
	 
		for(int j=0; j< 5; j++) {
			System.out.print(c[j] + " ");
			for(int i=0; i<5; i++) {
				int rannum =  (int)(Math.random()*(45-1+1) + 1);
				if(로또.nodup(num, rannum))
					rannum =  (int)(Math.random()*(45-1+1) + 1);
					
				num[i] =
				System.out.print(num[i] + " ");
				
			}
			System.out.println();
			
		}

	}

}
