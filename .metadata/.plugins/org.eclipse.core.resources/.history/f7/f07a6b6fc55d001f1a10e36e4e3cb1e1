package ex01;

import java.util.Random;

public class 연습문제 {
	
	public static void main(String[] args) {
		
		//int형 배열 참조변수 nArr 설정
		int nArr[] = new int[5];
		int l = nArr.length;
		
		//배열 초기화 -> 값 범위는 1~100 => 랜덤함수 이용해서
		for(int i=0; i<l; i++)
			nArr[i] = (int)(Math.random()*100) + 1;
		
		for(int i=0; i<l; i++)
			//System.out.printf("nArr[%d] = %d\n", i, nArr[i]);
			System.out.println(nArr[i] + ", ");
	}

}
