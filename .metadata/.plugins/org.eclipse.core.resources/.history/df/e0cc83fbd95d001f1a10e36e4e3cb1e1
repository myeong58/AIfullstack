package ex01;

import java.util.Random;

public class 로또 {

	public static void main(String[] args) {
		int num[] = new int[5];
		char c[] = {'A', 'B', 'C', 'D', 'E'};
		
		for(int j=0; j< 5; j++) {
			System.out.print(c[j] + " ");
			for(int i=0; i<5; i++) {
				num[i] = (int)(Math.random()*(45-1+1) + 1);
				System.out.print(num[i] + " ");
				
			}
			System.out.println();
			
		}

	}

}
