package ex01;

import java.util.Random;

public class 로또3 {
	public static void main(String[] args) {
		//로또 -> 5게임
		
		int lotto[] = new int[45];
		
		Random random = new Random();
		
		for(int i=0; i<45; i++)
			lotto[i] = i+1;
		
		for(int i=0; i<45; i++) {
			int tmp = random.nextInt(45);
			tmp = lotto[i];
			lotto[i] = lotto[tmp];
			lotto[tmp] = tmp;
		}
		
		for(int i=0; i<5; i++) {
			for(int j=0; j<6; j++)
				System.out.println(lotto[j] + " ");
			System.out.println();
		}
		

		
	}
}
