package ex01;

import java.util.Random;

public class 로또2 {
	public static void main(String[] args) {
		int num[] = new int[6];
		char c[] = {'A', 'B', 'C', 'D', 'E'};
		boolean check = true;
		int rannum = 1;
		
		for(int j=0; j< 5; j++) {
			System.out.print(c[j] + " ");
			for(int i=0; i<6; i++) {
				check = true;
				while(check) {
					rannum = (int)(Math.random()*45) + 1;
					for(int k=0; k<i; k++) {
						if(rannum == num[k]) 
							break;
					}

				}
				num[i] = rannum;
				check = false;
				
			
				System.out.print(num[i] + " ");
				
			}
			System.out.println();
			
		}

	}

}
