package ex01;

import java.util.Random;

public class 로또2 {
	public static void main(String[] args) {
		int num[] = new int[6];
		char c[] = {'A', 'B', 'C', 'D', 'E'};
		boolean check = true;

		for(int j=0; j< 5; j++) {
			System.out.print(c[j] + " ");
			for(int i=0; i<6; i++) {
				check = false;
				while(true) {
					int rannum = (int)(Math.random()*45) + 1;
					for(int k=0; k<i; k++) {
						if(rannum == num[k]) 
							check = true;
							i--;
							break;
					}
					if(check) {
						num[i] = rannum;
						System.out.print(num[i] + " ");
					}
	
				}				
					
			}
			System.out.println();
		}

	}

}
