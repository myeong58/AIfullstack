package ex01;

import java.util.Random;

public class 로또_연습 {
	public static void main(String[] args) {
		int lotto[] = new int[6];
		
		Random random = new Random();
		int l = lotto.length;
		
		for(int k=0; k<5; k++) {
			
			for(int i=0; i<l; i++) {
				lotto[i] = random.nextInt(45)+1;
				for(int j=0; j<i; j++) {
					if(lotto[i] == lotto[j])
						i--;
				}
			}
			for(int i=0; i<l; i++) {
				System.out.printf("%d, ",lotto[i]);
			}
			System.out.println();
		}
		
	}
}
